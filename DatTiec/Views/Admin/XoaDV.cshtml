@model DatTiec.DICHVU

@{
    ViewBag.Title = "XoaDV";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<<div class="content">
	<div class="container-fluid">
		<div class="row">
			<div class="col-md-12">
				<div class="card strpied-tabled-with-hover">
					<div class="card-header ">
						<h4 class="card-title">XÓA DỊCH VỤ</h4>

					</div>
					<div>
						<hr />
						<dl class="dl-horizontal">

							<dt>
								Tên Dịch Vụ
							</dt>

							<dd>
								@Html.DisplayFor(model => model.TENDICHVU)
							</dd>

							<dt>
								Giá Dịch Vụ
							</dt>

							<dd>
								@Html.DisplayFor(model => model.GIADV)
							</dd>

						</dl>

						@using (Html.BeginForm())
						{
							@Html.AntiForgeryToken()

							<div class="form-actions no-color">
								<input type="submit" value="Xóa" class="btn btn-default" /> |
								@Html.ActionLink("Trở về", "showDV")
							</div>
						}
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
